    "prettier": "npx prettier --write .",
    "test": "npm run lint && npm run test:unit",
    "test:e2e": "npx jest e2e.test --config jest.config.e2e.js --silent",
    "test:int": "npx jest int.test --config jest.config.int.js --silent",
    "test:unit": "npx jest unit.test --config jest.config.js --silent"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@swc/core": "^1.3.65",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.2",
    "esbuild": "^0.17.19",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "serverless": "^3.32.2",
    "serverless-appsync-plugin": "^1.14.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.45.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-prune-plugin": "^2.0.2"
